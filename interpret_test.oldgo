package userio
import (
  "testing"

  "github.com/stretchr/testify/suite"
)

type UserIOInterpretSuite struct {
  suite.Suite
}

func TestUserIOInterpret(t *testing.T) {
  suite.Run(t, new(UserIOInterpretSuite))
}

func (suite *UserIOInterpretSuite) TestRequestIsAcceptable() {
  testCases := []*UserRequest{
    &UserRequest{ Raw: "find id cloud by email" },
    &UserRequest{ Raw: "find id cloud by email in engineering for babs" },
    &UserRequest{ Raw: "doit" },
    &UserRequest{ Raw: "cancel" },
  }

  for _, testCase := range testCases {
    suite.True(testCase.requestIsAcceptable())
  }
}
